{
  "version": "1.0.0-beta.8",
  "name": "@kanvas/phoenix-rebirth",
  "description": "Client and ui kit",
  "files": [
    "dist/**/*.{js,mjs,d.ts,d.mts,css}"
  ],
  "exports": {
    "./lib/*": {
      "import": {
        "types": "./dist/lib/*.d.mts",
        "default": "./dist/lib/*.mjs"
      },
      "require": {
        "types": "./dist/lib/*.d.ts",
        "default": "./dist/lib/*.js"
      }
    },
    "./store": {
      "import": {
        "types": "./dist/store/index.d.mts",
        "default": "./dist/store/index.mjs"
      },
      "require": {
        "types": "./dist/store/index.d.ts",
        "default": "./dist/store/index.js"
      }
    },
    "./icons": {
      "import": {
        "types": "./dist/icons/index.d.mts",
        "default": "./dist/icons/index.mjs"
      },
      "require": {
        "types": "./dist/icons/index.d.ts",
        "default": "./dist/icons/index.js"
      }
    },
    "./views/*": {
      "import": {
        "types": "./dist/views/*.d.mts",
        "default": "./dist/views/*.mjs"
      },
      "require": {
        "types": "./dist/views/*.d.ts",
        "default": "./dist/views/*.js"
      }
    },
    "./hooks": {
      "import": {
        "types": "./dist/hooks/index.d.mts",
        "default": "./dist/hooks/index.mjs"
      },
      "require": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks/index.js"
      }
    },
    "./ui/*": {
      "import": {
        "types": "./dist/ui/*.d.mts",
        "default": "./dist/ui/*.mjs"
      },
      "require": {
        "types": "./dist/ui/*.d.ts",
        "default": "./dist/ui/*.js"
      }
    },
    "./blocks/*": {
      "import": {
        "types": "./dist/blocks/*.d.mts",
        "default": "./dist/blocks/*.mjs"
      },
      "require": {
        "types": "./dist/blocks/*.d.ts",
        "default": "./dist/blocks/*.js"
      }
    },
    "./global.css": "./dist/global.css",
    "./tour.css": "./dist/lib/tour.css",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "prepare": "pnpm build",
    "build": "tsup --env.NODE_ENV production && node -e \"require('fs').copyFileSync('package.json', 'dist/package.json')\"",
    "watch": "NODE_OPTIONS='--max-old-space-size=16384' tsup --watch",
    "add": "pnpm dlx shadcn@latest add"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyanvasu/phoenix-rebirth.git"
  },
  "author": "Erick Sosa",
  "contributors": [
    "Erick Sosa"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyanvasu/phoenix-rebirth/issues"
  },
  "homepage": "https://github.com/kyanvasu/phoenix-rebirth#readme",
  "devDependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^18.3.24",
    "cookies-next": "^4.3.0",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "yup": "^1.7.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^22.18.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "driver.js": "^1.3.6",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.453.0",
    "next": "15.0.1",
    "next-themes": "^0.3.0",
    "postcss": "^8.5.6",
    "radix-ui": "^1.4.3",
    "react-day-picker": "^9.9.0",
    "react-hook-form": "^7.62.0",
    "react-phone-number-input": "^3.4.12",
    "react-resizable-panels": "^2.1.9",
    "react-store-js": "^1.0.0",
    "react-use": "^17.6.0",
    "recharts": "2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.12",
    "tsup": "^8.5.0",
    "tw-animate-css": "^1.3.7",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  }
}
